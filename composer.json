{
  "name": "sypets/migrate2composer",
  "description": "This extension dumps information about currently loaded extensions to screen to be used as composer.json or to run composer commands.",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "TYPO3",
    "TYPO3 extension",
    "composer",
    "migrate to composer",
    "migrate2composer",
    "cli",
    "migrate"
  ],
  "authors": [
    {
      "name" : "Sybille Peters",
      "email": "sypets@gmx.de",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^10.4.38"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "migrate2composer"
    }
  }
}
